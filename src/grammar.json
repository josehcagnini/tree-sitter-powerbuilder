{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "powerbuilder",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "HA$PBExportHeader$"
            },
            {
              "type": "SYMBOL",
              "name": "class_name"
            },
            {
              "type": "STRING",
              "value": "."
            },
            {
              "type": "SYMBOL",
              "name": "class_type"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "forward_types"
        },
        {
          "type": "SYMBOL",
          "name": "global_class_properties"
        },
        {
          "type": "SYMBOL",
          "name": "class_variables"
        },
        {
          "type": "SYMBOL",
          "name": "forward_prototypes"
        }
      ]
    },
    "class_name": {
      "type": "SYMBOL",
      "name": "identifier"
    },
    "class_type": {
      "type": "SYMBOL",
      "name": "identifier"
    },
    "forward_types": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "forward"
        },
        {
          "type": "SYMBOL",
          "name": "class_inherit_from"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "forward_type"
          }
        },
        {
          "type": "STRING",
          "value": "end forward"
        }
      ]
    },
    "class_inherit_from": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "global type"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": "from"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": "end type"
        }
      ]
    },
    "forward_type": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "type"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": "from"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": "within"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": "end type"
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9_]+"
    },
    "global_class_properties": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "global type"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": "from"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "class_properties"
        },
        {
          "type": "STRING",
          "value": "end type"
        }
      ]
    },
    "class_variables": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "type variables"
        },
        {
          "type": "SYMBOL",
          "name": "class_properties"
        },
        {
          "type": "STRING",
          "value": "end variables"
        }
      ]
    },
    "forward_prototypes": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "forward prototypes"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "function_prototype"
          }
        },
        {
          "type": "STRING",
          "value": "end prototypes"
        }
      ]
    },
    "function_prototype": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "SYMBOL",
          "name": "function_parameters"
        }
      ]
    },
    "function_parameters": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "function_parameter"
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "STRING",
                    "value": ","
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "function_parameter": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "ref"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "type"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "class_properties": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "property"
      }
    },
    "property": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "type"
        },
        {
          "type": "SYMBOL",
          "name": "property_name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "="
                },
                {
                  "type": "SYMBOL",
                  "name": "value"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "type": {
      "type": "PATTERN",
      "value": "\\w+"
    },
    "property_name": {
      "type": "PATTERN",
      "value": "\\w+"
    },
    "value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "string_literal"
        },
        {
          "type": "SYMBOL",
          "name": "integer"
        },
        {
          "type": "SYMBOL",
          "name": "decimal"
        },
        {
          "type": "SYMBOL",
          "name": "boolean_literal"
        }
      ]
    },
    "string": {
      "type": "PATTERN",
      "value": "\"([^\"\\\\]|\\\\.)*\""
    },
    "integer": {
      "type": "PATTERN",
      "value": "\\d+"
    },
    "decimal": {
      "type": "PATTERN",
      "value": "\\d+\\.\\d+"
    },
    "boolean_literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "true"
        },
        {
          "type": "STRING",
          "value": "false"
        },
        {
          "type": "STRING",
          "value": "null"
        }
      ]
    },
    "string_literal": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "string_literal_content"
              },
              {
                "type": "SYMBOL",
                "name": "escape_sequence"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "string_literal_content": {
      "type": "CHOICE",
      "members": [
        {
          "type": "IMMEDIATE_TOKEN",
          "content": {
            "type": "PREC",
            "value": 1,
            "content": {
              "type": "PATTERN",
              "value": "[^\"\\\\\\n]+"
            }
          }
        },
        {
          "type": "PREC",
          "value": 2,
          "content": {
            "type": "IMMEDIATE_TOKEN",
            "content": {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "\\"
                },
                {
                  "type": "PATTERN",
                  "value": "[^abefnrtv'\\\"\\\\\\?0]"
                }
              ]
            }
          }
        }
      ]
    },
    "escape_sequence": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "PATTERN",
            "value": "\\\\x[0-9a-fA-F]{2,4}"
          },
          {
            "type": "PATTERN",
            "value": "\\\\u[0-9a-fA-F]{4}"
          },
          {
            "type": "PATTERN",
            "value": "\\\\U[0-9a-fA-F]{8}"
          },
          {
            "type": "PATTERN",
            "value": "\\\\[abefnrtv'\\\"\\\\\\?0]"
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
